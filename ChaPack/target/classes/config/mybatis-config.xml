<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- MemberMapper.xml에 대한 정보 입력 -->
<!-- [DB연결 사용법] 만든 DTO와 Mapper정의하기 -->

<configuration>
	<typeAliases>
		<typeAlias type="com.spring.ex.Dto.AdminDto" alias="AdminDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.CartDto" alias="CartDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.Community_Board_Comment_Dto" alias="CommunityBoardCommentDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.Community_Board_Dto" alias="CommunityBoardDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.Market_Comment_Dto" alias="MarketCommentDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.MarketDto" alias="MarketDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.MemberDto" alias="MemberDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.OrderDto" alias="OrderDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.ReservationDto" alias="ReservationDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.Vanlife_Board_Comment_Dto" alias="VanlifeBoardCommentDTO"></typeAlias>
		<typeAlias type="com.spring.ex.Dto.Vanlife_Board_Dto" alias="VanlifeBoardDTO"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/MemberMapper.xml" />
		<mapper resource="mapper/BoardMapper.xml" />
	</mappers>
</configuration>
